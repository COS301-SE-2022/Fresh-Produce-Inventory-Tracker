{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@fresh-produce-inventory-tracker//api/authentication//api": [
        "libs/api/authentication/api/src/index.ts"
      ],
      "@fresh-produce-inventory-tracker//api/authentication//repository": [
        "libs/api/authentication/repository/src/index.ts"
      ],
      "@fresh-produce-inventory-tracker//api/authentication//service": [
        "libs/api/authentication/service/src/index.ts"
      ],
      "@fresh-produce-inventory-tracker/api/image/api": [
        "libs/api/image/api/src/index.ts"
      ],
      "@fresh-produce-inventory-tracker/api/image/repository": [
        "libs/api/image/repository/src/index.ts"
      ],
      "@fresh-produce-inventory-tracker/api/image/service": [
        "libs/api/image/service/src/index.ts"
      ],
      "@fresh-produce-inventory-tracker/api/prisma/shared": [
        "libs/api/prisma/shared/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
